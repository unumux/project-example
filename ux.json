{
    "scss": {
        "src": [
            "styles/**/*.scss",             
            "node_modules/@unumux/colonial-branding-alpha/@(base|components|mixins)/**/_*.scss", 
            "node_modules/@unumux/colonial-branding-alpha/@(base|components|mixins)/**/_*.scss", 
            "node_modules/@unumux/colonial-branding-alpha/_*.scss",
            "node_modules/@unumux/theme-enterprise-high-contrast/@(base-elements|colors|fonts)/**/_*.scss",
            "node_modules/@unumux/theme-enterprise-high-contrast/_*.scss"            
        ],
        "dest": "styles"
    },
    "js": {
        "src": "scripts/**/*.js",
        "dest": "scripts",
        "main": "scripts/site.js"
    },
    "watch": [
        "index.html"
    ],
    "server": true,
    "compileJs": true,
    "proxy": false
}